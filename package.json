{
  "name": "projectz-front",
  "version": "1.0.0",
  "description": "ProjectZ Front-end based on NuxtJS",
  "author": "H&amp;E",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
